form.address-content(layout="column", name="addressForm")
    md-input-container.sk-input-box.sk-input-non-floating
        label Street Address 1
        input(name="address1", type="text", ng-model="address1")
    md-input-container.sk-input-box.sk-input-non-floating
        label Street Address 2
        input(name="address2", type="text", ng-model="address2")
    md-input-container.sk-input-box.sk-input-non-floating
        label city
        input(name="city", type="text", ng-model="city")
    md-input-container.sk-input-box.sk-input-non-floating
        label State/Province/Region
        input(name="region", type="text", ng-model="region")
    md-input-container.sk-input-box.sk-input-non-floating
        label Zip/Postal Code
        input(name="code", type="text", ng-model="code")
    md-input-container.sk-input-box.sk-input-non-floating
        label Country
        md-select.sk-select(name="country", ng-model="country", ng-required="true")
            md-option(ng-value="item.name", ng-repeat="(key, item) in countryList") {{item.name}}
