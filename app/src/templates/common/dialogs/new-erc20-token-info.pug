md-dialog.info-container(aira-lable="{{title}}")
    .info-wrapper
        .info-title
            span {{ title }}
        .info-body
            div Symbol: {{ symbol }}
            div Balance: {{ balance }}

        md-button.default.outline-blue(ng-click="cancel($event)", sk-button-loading="reloadTokensPrimise.$$state.status")
            span.primary OK
